pipeline {
    agent any

    stages {
        stage('Checkout Code') {
            steps {
                git 'https://github.com/Emmy-S-300/git-tutorial3'
            }
        }
        stage('Build'){
            steps {
                sh 'echo "building the app"'
            }
        }
        stage('Test'){
            steps {
                sh 'echo "Deploying the app"'
            }
        }
        stage('Deploy'){
            steps {
                sh 'echo "deploying"'
            }
        }
    }
}
